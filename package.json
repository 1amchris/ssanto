    "flattenizer": "^1.1.1",
    "i18next": "^21.6.10",
    "jquery": "^3.6.0",
    "leaflet": "^1.7.1",
    "leaflet-simple-map-screenshoter": "^0.4.5",
    "net": "^1.0.2",
    "react": "^17.0.2",
