    "eject": "react-scripts eject",
    "start-web": "cross-env BROWSER=none npm run start:react",
    "start-electron": "electron .",
    "build:electron": "electron-packager . --platform=win32 --arch=x64 --icon=./public/favicon.ico --no-asar --prune=true --ignore='(README.md|tsconfig.json|.git*|.prettier*|.eslint*|requirements.txt|.husky|/node_modules|/public|/src)' --overwrite --out=./release",
    "prepare": "husky install"
  },
